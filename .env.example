# Veritabanı bağlantı bilgileri
DATABASE_URL=postgres://postgres:postgres@localhost:5432/serverdb

# Uygulama ayarları
PORT=5000
NODE_ENV=development

# JWT Ayarları
JWT_SECRET=your_jwt_secret_key_change_this
JWT_EXPIRES_IN=24h

# Admin kullanıcı ilk kurulum bilgileri
DEFAULT_ADMIN_USERNAME=admin
DEFAULT_ADMIN_PASSWORD=admin123

# Otomatik kurulum yapılandırması
AUTO_INITIALIZE_DB=true
